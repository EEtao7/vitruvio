%% getInverseDynamics

function jointTorque.(EEselection) = getInverseDynamics(q, meanCyclicMotionHipEE, robotConfig, config)

for j = 1:4
    
for i = 1:length(q.(EEselection).rotationalAcceleration(:,1))
   jointVel = q.(EEselection).rotationalVelocity(i,1:3);
   jointAccel = q.(EEselection).rotationalAcceleration(i,:);

   wrench = [0 0 0 meanCyclicMotionHipEE.LH.force(i,1:3)]; % need to rotate this force to base frame?
   fext = externalForce(robotConfig,'body4',wrench);
    
   jointTorque.(EEselection)(i,:) = inverseDynamics(robotConfig, config(i,:), jointVel, jointAccel); %, fext);
 end
jointTorque.(EEselection);
% inverseDynamics(robot, config)
